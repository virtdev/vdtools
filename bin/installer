#!/usr/bin/python

import sys
from vdtools.parser import parse
from vdtools.installer import install

def usage():
    print('usage: installer [uid] [path]')

if __name__ == '__main__':
    argc = len(sys.argv)
    if argc != 3:
        usage()
        sys.exit()
    uid = sys.argv[1]
    path = sys.argv[2]
    args = parse(path)
    install(uid, args)
