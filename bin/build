#!/usr/bin/python

import os
import sys
import commands

_name = commands.getoutput('readlink -f %s' % sys.argv[0])
_path = os.path.dirname(_name)
_dir = os.path.dirname(_path)
sys.path.append(_dir)

from lib.parser import Parser

def usage():
    print('usage: build [uid] [path]')

if __name__ == '__main__':
    argc = len(sys.argv)
    if argc != 3:
        usage()
        sys.exit()
    uid = sys.argv[1]
    path = sys.argv[2]
    parser = Parser(uid)
    print(str(parser.build(path)))
