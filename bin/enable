#!/usr/bin/python

import os
import sys
import commands

_bin = commands.getoutput('readlink -f %s' % sys.argv[0])
_path = os.path.dirname(_bin)
_dirname = os.path.dirname(_path)
sys.path.append(_dirname)

from lib.common import enable

def usage():
    print('usage: enable [name]')

if __name__ == '__main__':
    if len(sys.argv) != 2:
        usage()
        sys.exit()
    enable(sys.argv[1])
