#!/usr/bin/python

import os
import sys
import commands

_bin = commands.getoutput('readlink -f %s' % sys.argv[0])
_path = os.path.dirname(_bin)
_dirname = os.path.dirname(_path)
sys.path.append(_dirname)

from lib.parser import Parser

def usage():
    print('usage: evaluate [path]')
    
if __name__ == '__main__':
    argc = len(sys.argv)
    if argc != 2:
        usage()
        sys.exit()
    path = sys.argv[1]
    parser = Parser()
    print(parser.build(path))
